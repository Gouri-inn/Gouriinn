{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/menu",
      "destination": "/seva",
      "permanent": true
    },
    {
      "source": "/inventory.html",
      "destination": "/rooms",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ]
}
